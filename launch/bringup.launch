<?xml version="1.0"?>
<!--Install the following
-ros-kinetic-desktop-full
-ros-kinetic-lms1xx
-ros-kinetic-nmea-navsat-driver
-ros-kinetic-pid
-ros-kinetic-gps-common
-ros-kinetic-robot-pose-ekf
-->

<launch>

    <arg name="gps_com" default="/dev/ttyUSB0" />
    <!--Load the URDF and run Robot State Publisher-->
    <include file="$(find asv_bringup)/launch/minimal.launch" />

    <!--Sensor Launch files -->
    <include file="$(find asv_sensors)/launch/gps.launch">
        <arg name="gps_com" value="$(arg gps_com)" />
    </include>

    <node name="imu" pkg="asv_sensors" type="imu_comm.py" respawn="true" output="screen" />

<!-- All the sensors's nodes have been created here for sensor streams -->
